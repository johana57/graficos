{% extends 'PequivenSEIPBundle:Template:Developer/layoutWithoutMenuBlack.html.twig' %}

{% trans_default_domain 'PequivenSEIPBundle' %}

{% block stylesheets_base %}
    {{ parent() }}
    <link rel="stylesheet" href="{{ asset('bundles/pequivenseip/icons/imageMap.css') }}">
{% endblock %}

{% block body %}
{#<meta http-equiv="refresh" content="300;">#}
<div class="columns">        
        <div class="new-row three-columns twelve-columns-mobile">
            <div  ng-controller="displayCenterController" ng-init="renderChartVotoGeneralCircuito('displayVotoGeneral',{{ dataChartCircuto }})">
                <span id="displayVotoGeneral">
                </span>
            </div>
        </div>
        <div class="three-columns twelve-columns-mobile">
            <div  ng-controller="displayCenterController" ng-init="renderChartVotoGeneralCircuito('displayVotoGeneral1x10',{{ dataChartCircuto1x10 }})">
                <span id="displayVotoGeneral1x10">
                </span>
            </div>
        </div>
        <div class="one-column">
        </div>
        <div class="five-columns twelve-columns-mobile">
            <div  ng-controller="displayCenterController" ng-init="renderChartVotoGeneralHoursCircuito('displayHours',{{ dataHours }})">
                <span id="displayHours">
                </span>
            </div>
        </div>
</div>
<div class="columns"> 
    <div class="eight-columns twelve-columns-mobile" style="padding-top:0px;">
        <div  ng-controller="displayCenterController" ng-init="renderChartVotoGeneralCircuito5('displayCircuito5',{{ dataChartCircutoBarra }})">
            <span id="displayCircuito5">
            </span>
        </div>
    </div>
    <div class="four-columns twelve-columns-mobile" style="padding-top:0px;">
        <div  ng-controller="displayCenterController" ng-init="renderChartVotoGeneralCircuitoPoll('poll',{{ dataChartPoll }})">
            <span id="poll">
            </span>
        </div>
    </div>
</div>

{% endblock body %}


{% block stylesheets %}
    {{ parent() }}
    <link rel="stylesheet" href="{{ asset('bundles/tecnocreacionesvzlagovernment/template/developer/css/styles/agenda3860.css?v=1') }}">
    <link rel="stylesheet" href="{{ asset('bundles/tecnocreacionesvzlagovernment/template/developer/css/styles/dashboard3860.css?v=1') }}">
{% endblock stylesheets %}

{% block javascripts %}
    {{ parent() }}
    <script src="{{  asset('bundles/pequivenseip/fusioncharts/js/fusioncharts.js') }}"></script>
    <script src="{{  asset('bundles/pequivenseip/fusioncharts/js/themes/fusioncharts.theme.fint.js') }}"></script>
    <script src="{{  asset('bundles/pequivenseip/imageMapResizer/js/imageMapResizer.min.js') }}"></script>
{% endblock %}

{% block javascript %}
    {{ parent() }}
    <script type="text/javascript">
        $(document).ready(function () {
        });
    </script>
{% endblock %}