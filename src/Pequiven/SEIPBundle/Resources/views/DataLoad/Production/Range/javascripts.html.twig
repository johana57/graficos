<script type="text/javascript">
    $(document).ready(function(){
        var dateFormat = "dd-mm-yy";
        $( ".date-from").datepicker({
          changeMonth: false,
          numberOfMonths: 1,
          dateFormat: dateFormat
        });
        $( ".date-end").datepicker({
          changeMonth: false,
          numberOfMonths: 1,
          dateFormat: dateFormat
        });
    });
</script>